<SearchPage>:
	MDBoxLayout:
		orientation: 'vertical'
		
		MDBoxLayout:
			padding: dp(10)
			adaptive_height: True
			MDIconButton:
				icon: 'arrow-left'
				on_release: app.root.current = 'homepage'
				
			MDTextField:
				id: searchbox
				mode: 'round'
				hint_text: 'Search'
				size_hint_x: None
				width: dp(250)
				pos_hint: {'center_y':.5}
			MDIconButton:
				icon: 'magnify'
				on_release: app.search(searchbox)
		MDBoxLayout:
			MDScrollView:
				bar_width: 0
				
				MDBoxLayout:
					orientation: 'vertical'
					adaptive_height: True
					id: search_reaults